pipeline {
    agent { dockerfile true }
    stages {
        stage('Cloning') {
            steps {
                echo "Cloning the code fron Github"
                git branch: 'master', url: 'https://github.com/devops4solutions/CI-CD-using-Docker.git'
            }
        }

      
       stage('Test') {
            steps {
                sh 'node --version'
                sh 'svn --version'
            }
        }

      
       stage('Test') {
            steps {
                sh 'node --version'
                sh 'svn --version'
            }
        }

      
       stage('Test') {
            steps {
                sh 'node --version'
                sh 'svn --version'
            }
        }

      
      
      
      
      
      
    }
}
